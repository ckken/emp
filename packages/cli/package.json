{
  "name": "@empjs/cli",
  "version": "3.8.1",
  "description": "emp",
  "license": "MIT",
  "type": "module",
  "files": ["dist", "bin", "resource", "template", "client.d.ts", "emp-tsconfig.json"],
  "maintainers": ["xuhongbin", "ckken", "doerme"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/empjs/emp.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": ["rspack", "rust", "typescript"],
  "bin": {
    "emp": "./bin/emp.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./emp-tsconfig.json": "./emp-tsconfig.json"
  },
  "scripts": {
    "dev": "tsup --watch --env.ENV dev",
    "build": "tsup --env.ENV prod",
    "ph": "pnpm publish --no-git-checks"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Ken",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/default-gateway": "^7.2.1",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.8.7",
    "@types/react": "^18.3.2",
    "@types/watchpack": "^2.4.3",
    "@types/webpack": "^5.28.4",
    "@types/webpack-bundle-analyzer": "^4.6.2",
    "@types/webpack-sources": "^3.2.2",
    "tsup": "^8.4.0"
  },
  "dependencies": {
    "@rsdoctor/rspack-plugin": "1.1.5",
    "@rspack/core": "1.4.1",
    "@rspack/dev-server": "1.1.1",
    "@swc/helpers": "^0.5.17",
    "address": "^2.0.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "core-js": "^3.41.0",
    "cors": "^2.8.5",
    "default-gateway": "^7.2.2",
    "express": "^4.21.2",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.1",
    "gzip-size": "^7.0.0",
    "html-webpack-plugin": "5.6.3",
    "jiti": "2.0.0",
    "less-loader": "^12.2.0",
    "open": "^10.1.0",
    "sass-embedded": "1.89.2",
    "sass-loader": "^16.0.5",
    "ts-checker-rspack-plugin": "^1.1.4",
    "typescript-plugin-css-modules": "^5.1.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-chain": "^6.5.1"
  }
}
